`timescale 1ns/1ns        //定义时间单位
`define clk_cycle 2    //定义一个符号常量（参数型常量）

module exp21test;
reg F10M, RESET;
wire F500K_clk;

always #`clk_cycle F10M = ~F10M;

initial            //initial常用于仿真信号的给出
    begin
	RESET = 1;
	F10M = 0;
	#100 RESET = 0;
	#100 RESET = 1;
	#100000 $stop;       //系统任务，暂停仿真以便观察仿真波形
   end

  exp21  fdivision (.RESET(RESET),.F10M(F10M), .F500K(F500K_clk));  //调用被测试模块test.m
endmodule
